<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>新增车票</title>
</head>
<body>
	<h3>新增车票信息</h3>
	
	<form action="${pageContext.request.contextPath }/price_save.action?" method="post">
		<table>
		<tr>
			<td>票价率编号</td>
			<td><input type="text" name="id" value="11"></td>
		</tr>
		<tr>
			<td>车厢类型</td>
			<td><input type="text" name="traintype" value="TT"></td>
		</tr>
			<tr>
			<td>车箱车体类型</td>
			<td><input type="text" name="trainbodytype" value="XG"></td>
		</tr>
		</tr>
			<tr>
			<td>席位类型</td>
			<td><input type="text" name="seattype" value="XG"></td>
		</tr>
		</tr>
			<tr>
			<td>票价率</td>
			<td><input type="text" name="rate" value="XG"></td>
		</tr>
		</tr>
			<tr>
			<td>比例</td>
			<td><input type="text" name="scale" value="XG"></td>
		</tr>
		</tr>
			<td><input type="submit" value="新增"></td>
			<td><input type="reset" value="重置"></td>
		</tr>

	</table>
	</form>
</body>
</html>